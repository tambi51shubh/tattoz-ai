{
  "functions": {
    "src/app/api/generate/route.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "regions": ["bom1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache"
        }
      ]
    }
  ]
} 